  object Qpagos: TIBQuery
    Database = DM.DB_ANGLO
    Transaction = DM.IBTr_ANGLO
    Active = True
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select B.NOSSO_NUMERO,'
      '          B.ALUNO, '
      '          B.VENCIMENTO, '
      '          B.VALOR, '
      '          B.PAGAMENTO, '
      '          B.VALOR_PAGO, '
      '          B.TIPO_PG,'
      '          B.ANO_DV,'
      
        '          A.SACADO, A.NOME, A.CELULAR, A.SERIE, A.TURMA, A.ENDER' +
        'ECO, A.NUMERO, A.BAIRRO,A.CIDADE, A.UF, A.CEP'
      'from BLOQUETOS B join ALUNOS A on B.ALUNO = A.CODIGO'
      'where ((B.VENCIMENTO >= :inicio) and (B.VENCIMENTO <= :fim)) and'
      
        '            ((B.PAGAMENTO is null) and (B.ST = '#39'E'#39' and (B.VALOR ' +
        '> 0))) and (A.STATUS = '#39'A'#39')'
      ''
      'order by  A.NOME, B.VENCIMENTO,  B.NOSSO_NUMERO')
    Left = 664
    Top = 272
    ParamData = <
      item
        DataType = ftString
        Name = 'inicio'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'fim'
        ParamType = ptUnknown
      end>
