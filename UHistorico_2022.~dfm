    Top = 152
  end
  object QTemp: TIBQuery
    Database = DM.DB_ANGLO
    Transaction = DM.IBTr_ANGLO
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'select fo.nome as formacao, i.nome_intinerario as intinerario, i' +
        '.codigo, a.nome_area as area, d.nome as disciplina,'
      
        'h.codigo as aluno, h.nota1, h.ch1, h.nota2, h.ch2, h.nota3, h.ch' +
        '3'
      'from disciplinas d'
      'join for_int f on d.codigo = f.cod_diciplina'
      
        'join are_conhecimento a on f.are_conhecimento_id_area = a.id_are' +
        'a'
      'join intinerarios i on f.intinerarios_codigo = i.codigo'
      'join formacao fo on f.formacao_codigo = fo.codigo'
      'join hist_notas h on d.codigo = h.disciplina'
      'where h.codigo = :cod_aluno'
      'order by i.codigo, a.nome_area, d.nome')
    Left = 542
    Top = 152
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_aluno'
        ParamType = ptUnknown
      end>
    object QTempDISCIPLINA: TIBStringField
      FieldName = 'DISCIPLINA'
      Origin = '"DISCIPLINAS"."NOME"'
      Size = 40
    end
    object QTempAREA: TIBStringField
      DisplayWidth = 80
      FieldName = 'AREA'
      Origin = '"ARE_CONHECIMENTO"."NOME_AREA"'
      Size = 80
    end
    object QTempINTINERARIO: TIBStringField
